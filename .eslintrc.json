{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "global-require": "off",
    "react/prop-types": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": "off",
    "no-unused-vars": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
